@model SecretariatCard


<div class="col-md-4">
    <div class="secretariatCard">
        <div>
            <img class="secretariatImg" src="@ViewData["image_path"]" />
        </div>

        <h4>@ViewData["name"]</h4>
        <h5>@ViewData["position"]</h5>
        <p>
            @ViewData["bio"]
        </p>
        <br />
        <h6 class="secretariatEmail">@ViewData["email"]</h6>
    </div>
</div>